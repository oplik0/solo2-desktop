	"version": "0.2.0",
	"configurations": [
		{
			"type": "pwa-node",
			"request": "launch",
			"name": "Launch Dev Server",
			"skipFiles": [
				"<node_internals>/**"
			],
			"runtimeExecutable": "pnpm",
			"runtimeArgs": ["run", "dev"],
			"outFiles": [
				"${workspaceFolder}/.svelte-kit/**/*.js"
			],
			"console": "integratedTerminal"
		}
	]
}
